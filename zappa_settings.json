{
    "dev": {
        "app_function": "bobsled.app.app",
        "events": [
            {
                "function": "bobsled.cluster.autoscale",
                "expression": "rate(20 days)"
            },
            {
                "function": "bobsled.status.update_status",
                "expression": "rate(30 days)"
            }
        ],
        "apigateway_enabled": false,
        "async_resources": false,
        "keep_warm": false,
        "route53_enabled": false,
        "runtime": "python3.6",

        "aws_region": "us-east-1",
        "s3_bucket": "zappa-bobsled-jpt",
        "environment_variables": {
            "BOBSLED_STATUS_BUCKET": "jpt-zappa-status",
            "BOBSLED_CONFIG_PATH": "s3://zappa-bobsled-jpt/config.yaml"
        }
    }
}
