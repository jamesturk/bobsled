{
    "dev": {
        "app_function": "bobsled.app.app",
        "events": [
            {
                "function": "bobsled.cluster.autoscale",
                "expression": "rate(20 days)"
            },
            {
                "function": "bobsled.status.update_status",
                "expression": "rate(30 days)"
            }
        ],
        "apigateway_enabled": false,
        "async_resources": false,
        "keep_warm": false,
        "route53_enabled": false,
        "runtime": "python3.6",
        "extra_permissions": [
            {
            "Effect": "Allow",
            "Action": ["iam:PassRole"],
            "Resource": "*"
            },
            {
            "Effect": "Allow",
            "Action": [
                "ec2:RunInstances",
                "ec2:CreateTags",
                "ec2:DescribeInstances",
                "ec2:TerminateInstances"
            ],
            "Resource": "*"
            },
            {
            "Effect": "Allow",
            "Action": [
                "ecs:DescribeTaskDefinition",
                "ecs:RunTask",
                "ecs:DescribeTasks",
                "ecs:ListContainerInstances",
                "ecs:DescribeContainerInstances"
            ],
            "Resource": "*"
            }
        ],

        "aws_region": "us-east-1",
        "s3_bucket": "jpt-zappa",
        "environment_variables": {
            "BOBSLED_STATUS_BUCKET": "",
            "BOBSLED_CONFIG_PATH": "",
            "BOBSLED_SUBNET_ID": "",
            "BOBSLED_SECURITY_GROUP_ID": ""
        }
    }
}
